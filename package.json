{
  "name": "k3-comp",
  "version": "1.0.0",
  "description": "",
  "main": "k3-comp.js",
  "scripts": {
    "start": "npm-run-all --parallel watch-js watch-sass",
    "watch-js": "webpack-dev-server --config ./tasks/webpack.dev.js --mode development",
    "watch-sass": "sass -w scss/layermanager.scss ../origo/plugins/layermanager.css",
    "prewatch-sass": "sass scss/layermanager.scss ../origo/plugins/layermanager.css",
    "build": "webpack --config ./tasks/webpack.prod.js && npm run build-sass",
    "build-sass": "sass scss/layermanager.scss build/css/layermanager.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Andreas Fogelberg",
  "license": "MIT",
  "dependencies": {
    "npm-run-all2": "^8.0.4",
    "drag-drop-touch": "^1.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "compression-webpack-plugin": "^11.1.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "mini-css-extract-plugin": "^2.9.4",
    "sass": "^1.90.0",
    "webpack": "^5.101.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  }
}
